drop database if exists glueCircle;
create  database glueCircle CHARACTER SET 'utf8'   COLLATE 'utf8_general_ci' ;
use glueCircle;

CREATE TABLE `glueCircle`.`users_tb` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `username` VARCHAR(45) NOT NULL,
  `sex` VARCHAR(45) NOT NULL DEFAULT '男',
  `password` VARCHAR(45) NOT NULL,
  `mail` VARCHAR(45) NULL DEFAULT 'example@qq.com',
  `tel` VARCHAR(45) NULL DEFAULT '000000',
  `item` TEXT,
  PRIMARY KEY (`id`)
 );



CREATE TABLE `glueCircle`.`items_tb` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `theme` VARCHAR(200) NOT NULL,
  `content` VARCHAR(700) NOT NULL,
  `good` INT NULL DEFAULT 0,
  `bad` INT NOT NULL DEFAULT 0,
  `goodfrom` TEXT NULL,
  `badfrom` TEXT NULL,
  `author` INT NOT NULL,
  `buildTime` VARCHAR(64) NOT NULL DEFAULT 0,
  `pics` TEXT,
  `files` TEXT,
  `readNum` INT NULL DEFAULT 0,
  `issues` TEXT,
  PRIMARY KEY (`id`),
  INDEX `id_idx` (`author` ASC),
  FOREIGN KEY (`author`)
  REFERENCES `glueCircle`.`users_tb` (`id`)
  ON DELETE NO ACTION
  ON UPDATE NO ACTION);


CREATE TABLE `pics_tb` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(45) NOT NULL,
  `type` varchar(45) NOT NULL,
  `content` text NOT NULL,
  PRIMARY KEY (`id`)
);

CREATE TABLE `files_tb` (
  `id` int(11) NOT NULL,
  `name` varchar(45) NOT NULL,
  `type` varchar(45)  NOT NULL,
  `content` text NOT NULL,
  PRIMARY KEY (`id`)
);


CREATE TABLE `glueCircle`.`issues_tb` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `itemId` INT NOT NULL,
  `content` VARCHAR(45) NOT NULL,
  `peopleId` INT NOT NULL,
  `issueId` INT NULL DEFAULT '-1',
  `buildTime` VARCHAR(64) NOT NULL DEFAULT 0,
  PRIMARY KEY (`id`));



